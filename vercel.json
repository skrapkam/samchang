{
  "buildCommand": "yarn build",
  "installCommand": "yarn install --frozen-lockfile",
  "framework": "gatsby",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/gatsby-plugin-vercel-builder"
    }
  ],
  "env": {
    "SHARP_IGNORE_GLOBAL_LIBVIPS": "1",
    "NODE_OPTIONS": "--max_old_space_size=4096"
  }
}